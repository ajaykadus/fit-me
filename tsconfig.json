{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,  // Ensure this is set to false
      "jsx": "react-jsx" // Set to react-jsx for React 17+ or "react" for earlier versions
    },
    "include": ["src"]
  }